<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/relative_layout_delete"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    >
    <!-- Todo 다이얼로그 이미지 선아한테 요청하기 -->

    <TextView
        android:id="@+id/textView_delete_message"
        android:layout_width="240dp"
        android:layout_height="90dp"
        android:background="@drawable/list_delete_white_box"
        android:gravity="center"
        android:text="리스트를 삭제하시겠습니까?"
        android:textColor="#4a4a4a"
        android:textSize="16sp" />

    <ImageView
        android:id="@+id/image_view_cancel"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:layout_marginTop="90dp"
        android:src="@drawable/list_delete_cancel" />

    <ImageView
        android:id="@+id/image_view_delete"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:layout_marginTop="90dp"
        android:layout_marginLeft="120dp"
        android:src="@drawable/list_delete_delete" />

</FrameLayout>