<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.omjoonkim.doyouremember"
		  xmlns:android="http://schemas.android.com/apk/res/android">

	<application
		android:name=".common.AppApplication"
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity
			android:name=".app.SplashActivity"
			android:theme="@style/AppTheme.NoActionBar"/>
		<activity
			android:name=".app.MainActivity"
			android:theme="@style/AppTheme.NoActionBar">

			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
				<category android:name="android.intent.category.LAUNCHER"/>
				<data android:scheme="@string/kakao_scheme" android:host="@string/kakaolink_host" />
			</intent-filter>

		</activity>
		<activity
			android:name=".app.frequentlyusedaccount.registeraccount.RegisterFrequentlyUsedAccountActivity"
			android:label=""
			android:theme="@style/AppTheme.NoActionBar">

			<!--<intent-filter>-->
			<!--<action android:name="android.intent.action.MAIN" />-->

			<!--<category android:name="android.intent.category.LAUNCHER" />-->
			<!--</intent-filter>-->

		</activity>
		<activity
			android:name=".app.myaccount.registermyaccount.RegisterMyAccountActivity"
			android:label=""
			android:theme="@style/AppTheme.NoActionBar">
		</activity>

		<meta-data android:name="com.kakao.sdk.AppKey" android:value="@string/kakao_app_key"/>
	</application>

</manifest>